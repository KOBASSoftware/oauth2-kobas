{
  "name": "kobas/oauth2-kobas",
  "description": "Kobas OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Sean McNamara",
      "email": "me@seanmcn.com",
      "homepage": "https://github.com/seanmcn"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "kobas"
  ],
  "require": {
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.4.0",
    "mockery/mockery": "~1.2.0",
    "squizlabs/php_codesniffer": "~3.3.2"
  },
  "autoload": {
    "psr-4": {
      "Kobas\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kobas\\OAuth2\\Client\\Test\\": "test/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}